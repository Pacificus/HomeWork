package com.hillel;

public class Task{
	public static void main(String...args){
		if (args.length == 0){
			System.out.println("No data or incorrect input...???");
			return ;
		}
		int num = Integer.parseInt(args[0]);
		boolean event = (num%2 == 0) ? true : false;
		if (event) System.out.println(num + " is " + "event");
		else System.out.println(num + " is " + "odd");
	}
}
